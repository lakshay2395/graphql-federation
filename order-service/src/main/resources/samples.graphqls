query Query($getOrderByIdOrderId: ID!) {
    getOrderById(orderId: $getOrderByIdOrderId) {
        customer {
            emailId
            firstName
            id
            lastName
            profilePicURL
        }
        id
        products {
            cost
            id
            name
            type
        }
        status
    }
}

